.Dd December 22, 2006
.Dt MAKEIPLCOOKIE 9
.Os
.\" ------------------------------------------------------------
.Sh NAME
.Nm makeiplcookie
.Nd compose an ipl cookie
.\" ------------------------------------------------------------
.Sh SYNOPSIS
.In sys/param.h
.\" - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
.Ft ipl_cookie_t
.Fn makeiplcookie \
"ipl_t ipl"
.\" ------------------------------------------------------------
.Sh DESCRIPTION
.Fn makeiplcookie
composes a cookie which can be fed into
.Fa splraiseipl .
.Fa ipl
should be one of
.Dv IPL_
constants.
.\" ------------------------------------------------------------
.Sh RETURN VALUES
.Fn makeiplcookie
returns a composed cookie.
.\" ------------------------------------------------------------
.Sh SEE ALSO
.Xr spl 9 ,
.Xr splraiseipl 9
