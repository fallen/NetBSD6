#: src/envsubst.c:181
#, c-format, no-wrap
msgid "Substitutes the values of environment variables.\n"
msgstr "Vervangt de waarden van omgevingsvariabelen.\n"

#: src/envsubst.c:185
#, c-format, no-wrap
msgid "Operation mode:\n"
msgstr "Werkingsmodus:\n"

#: src/envsubst.c:188
#, c-format, no-wrap
